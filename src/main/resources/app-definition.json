{
  "template-version": "2022-03-31",
  "source-locations": [
    {
      "source-id": "s3-source",
      "source-type": "s3",
      "properties": {
        "s3-bucket": "${S3_BUCKET}",
        "s3-key-prefix": "${S3_PREFIX}"
      }
    }
  ],
  "output-locations": [
    {
      "output-id": "s3-output",
      "output-type": "s3",
      "properties": {
        "s3-bucket": "${S3_BUCKET}",
        "s3-key-prefix": "MODERNIZED_CODE/"
      }
    }
  ],
  "executives": [
    {
      "name": "my-engine",
      "type": "bluage",
      "version": "3.10.0"
    }
  ]
}